<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="coder1" class="com.sumonsarder.cse.XML_based_setter_injection.model.Coder">
        <property name="id" value="1001"></property>
        <property name="name" value="Md. Sumon Sarder"></property>
        <property name="language" value="Java Spring Boot"></property>
        <property name="computer" ref="computer1"></property>
    </bean>
    <bean id="computer1" class="com.sumonsarder.cse.XML_based_setter_injection.model.Computer">
        <property name="brand" value="HR"></property>
    </bean>

</beans>